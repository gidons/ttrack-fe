{
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/api/*", "/static/*"]
	},
  "routes": [
    {"route":"/login", "redirect": "/.auth/github/login"},
    {"route":"/logout", "redirect": "/.auth/logout"},
    {
      "route": "/songs*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/",
      "statusCode": 200
    }
  }
}
