{
	"navigationFallback": {
		"rewrite": "/index.html",
		"exclude": ["/api/*", "/static/*"]
	},
  "routes": [
    {
      "route": "/songs*",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/songs/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": {
      "redirect": "/",
      "statusCode": 200
    }
  }
}